<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="130dp"
                android:height="28dp"/>
            <solid
                android:color="@color/switchBackground"/>
            <stroke
                android:width="1dp"
                android:color="@color/switchBackground"/>
            <corners android:radius="120dp" />
        </shape>
    </item>
    <item android:drawable="@drawable/ic_baseline_coin"
        android:right="90dp" android:width="16dp" android:height="16dp"
        android:gravity="center"/>
    <item android:drawable="@drawable/ic_baseline_laundry"
        android:left="90dp" android:width="16dp" android:height="16dp"
        android:gravity="center"/>
</layer-list>